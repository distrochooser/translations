{
    "app-store-gui-selectable_description": "\u0398\u03ad\u03bb\u03c9 \u03bd\u03b1 \u03b5\u03b3\u03ba\u03b1\u03c4\u03b1\u03c3\u03c4\u03ae\u03c3\u03c9 \u03c4\u03b1 \u03c0\u03c1\u03bf\u03b3\u03c1\u03ac\u03bc\u03bc\u03b1\u03c4\u03ac \u03bc\u03bf\u03c5 \u03c7\u03c1\u03b7\u03c3\u03b9\u03bc\u03bf\u03c0\u03bf\u03b9\u03ce\u03bd\u03c4\u03b1\u03c2 \u03ad\u03bd\u03b1 \u03b5\u03af\u03b4\u03bf\u03c2 \" \u039a\u03b1\u03c4\u03ac\u03c3\u03c4\u03b7\u03bc\u03b1 \u03b5\u03c6\u03b1\u03c1\u03bc\u03bf\u03b3\u03ce\u03bd\".",
    "no-systemd-selectable_description": "\u0398\u03ad\u03bb\u03c9 \u03bd\u03b1 \u03b1\u03c0\u03bf\u03c6\u03cd\u03b3\u03c9 \u03c4\u03bf systemd",
    "manuals-selectable_description": "\u039c\u03c0\u03bf\u03c1\u03ce \u03bd\u03b1 \u03bb\u03cd\u03c3\u03c9 \u03c4\u03b1 \u03c0\u03c1\u03bf\u03b2\u03bb\u03ae\u03bc\u03b1\u03c4\u03ac \u03bc\u03bf\u03c5 \u03c7\u03c1\u03b7\u03c3\u03b9\u03bc\u03bf\u03c0\u03bf\u03b9\u03ce\u03bd\u03c4\u03b1\u03c2 \u03bf\u03b4\u03b7\u03b3\u03bf\u03cd\u03c2 \u03b1\u03c0\u03cc \u03c3\u03b5\u03bb\u03af\u03b4\u03b5\u03c2 wiki \u03ae \u03b5\u03ba\u03c0\u03b1\u03b9\u03b4\u03b5\u03c5\u03c4\u03b9\u03ba\u03ce\u03bd \u03b2\u03af\u03bd\u03c4\u03b5\u03bf",
    "need-human-help-selectable_description": "\u03a0\u03c1\u03bf\u03c4\u03b9\u03bc\u03ce \u03bd\u03b1 \u03b6\u03b7\u03c4\u03ac\u03c9 \u03b2\u03bf\u03ae\u03b8\u03b5\u03b9\u03b1 \u03b1\u03c0\u03cc \u03ac\u03bb\u03bb\u03bf\u03c5\u03c2",
    "license-open-source-selectable_description": "\u0398\u03ad\u03bb\u03c9 \u03bd\u03b1 \u03c7\u03c1\u03b7\u03c3\u03b9\u03bc\u03bf\u03c0\u03bf\u03b9\u03ce \u03b5\u03bb\u03b5\u03cd\u03b8\u03b5\u03c1\u03b5\u03c2 \u03ac\u03b4\u03b5\u03b9\u03b5\u03c2 \u03cc\u03c3\u03bf \u03c4\u03bf \u03b4\u03c5\u03bd\u03b1\u03c4\u03cc\u03bd \u03c0\u03b5\u03c1\u03b9\u03c3\u03c3\u03cc\u03c4\u03b5\u03c1\u03bf.",
    "proprietary-okay-when-working-selectable_description": "\u0395\u03af\u03bc\u03b1\u03b9 \u03b5\u03bd\u03c4\u03ac\u03be\u03b5\u03b9 \u03bc\u03b5 \u03bc\u03b7-\u03b5\u03bb\u03b5\u03cd\u03b8\u03b5\u03c1\u03b5\u03c2 \u03ac\u03b4\u03b5\u03b9\u03b5\u03c2 \u03b5\u03c6\u03cc\u03c3\u03bf\u03bd \u03c4\u03bf \u03c3\u03cd\u03c3\u03c4\u03b7\u03bc\u03ac \u03bc\u03bf\u03c5 \u03bb\u03b5\u03b9\u03c4\u03bf\u03c5\u03c1\u03b3\u03b5\u03af",
    "many-preselections-selectable_description": "\u0398\u03ad\u03bb\u03c9 \u03bd\u03b1 \u03c7\u03c1\u03b7\u03c3\u03b9\u03bc\u03bf\u03c0\u03bf\u03b9\u03ae\u03c3\u03c9 \u03c4\u03b9\u03c2 \u03c0\u03c1\u03bf\u03b5\u03c0\u03b9\u03bb\u03b5\u03b3\u03bc\u03ad\u03bd\u03b5\u03c2 \u03c0\u03c1\u03bf\u03ba\u03b1\u03b8\u03bf\u03c1\u03b9\u03c3\u03bc\u03ad\u03bd\u03b5\u03c2 \u03b5\u03c0\u03b9\u03bb\u03bf\u03b3\u03b5\u03c2 \u03c3\u03c4\u03bf\u03bd \u03b2\u03bf\u03b7\u03b8\u03cc \u03b5\u03b3\u03ba\u03b1\u03c4\u03ac\u03c3\u03c4\u03b1\u03c3\u03b7\u03c2",
    "commandline-setup-selectable_description": "\u0398\u03ad\u03bb\u03c9 \u03bd\u03b1 \u03b5\u03c0\u03b9\u03bb\u03ad\u03be\u03c9 \u03c4\u03b9\u03c2 \u03c1\u03c5\u03b8\u03bc\u03af\u03c3\u03b5\u03b9\u03c2 \u03bc\u03cc\u03bd\u03bf\u03c2 \u03bc\u03bf\u03c5",
    "many-choices-gui-selectable_description": "\u0398\u03ad\u03bb\u03c9 \u03bd\u03b1 \u03b4\u03b9\u03b1\u03bc\u03bf\u03c1\u03c6\u03ce\u03c3\u03c9 \u03cc\u03c3\u03bf \u03c4\u03bf \u03b4\u03c5\u03bd\u03b1\u03c4\u03cc\u03bd \u03c0\u03b5\u03c1\u03b9\u03c3\u03c3\u03cc\u03c4\u03b5\u03c1\u03b5\u03c2 \u03c1\u03c5\u03b8\u03bc\u03af\u03c3\u03b5\u03b9\u03c2 \u03c7\u03c1\u03b7\u03c3\u03b9\u03bc\u03bf\u03c0\u03bf\u03b9\u03ce\u03bd\u03c4\u03b1\u03c2 \u03b3\u03c1\u03b1\u03c6\u03b9\u03ba\u03ad\u03c2 \u03b5\u03c6\u03b1\u03c1\u03bc\u03bf\u03b3\u03ad\u03c2",
    "no-linux-contact-selectable_description": "\u0388\u03c7\u03c9 \u03b5\u03bb\u03ac\u03c7\u03b9\u03c3\u03c4\u03b5\u03c2 \u03ae \u03ba\u03b1\u03b8\u03cc\u03bb\u03bf\u03c5 \u03b3\u03bd\u03ce\u03c3\u03b5\u03b9\u03c2 \u03c3\u03c7\u03b5\u03c4\u03b9\u03ba\u03ac \u03bc\u03b5 \u03c4\u03bf Linux",
    "already-used-selectable_description": "\u0388\u03c7\u03c9 \u03ae\u03b4\u03b7 \u03c7\u03c1\u03b7\u03c3\u03b9\u03bc\u03bf\u03c0\u03bf\u03b9\u03ae\u03c3\u03b5\u03b9 \u03c4\u03bf Linux \u03b3\u03b9\u03b1 \u03bf\u03c1\u03b9\u03c3\u03bc\u03ad\u03bd\u03bf\u03c5\u03c2 \u03c3\u03ba\u03bf\u03c0\u03bf\u03cd\u03c2",
    "knowledge-present-selectable_description": "\u0388\u03c7\u03c9 \u03ba\u03b1\u03bb\u03ae \u03b3\u03bd\u03ce\u03c3\u03b7 \u03c4\u03bf\u03c5 \u03bb\u03b5\u03b9\u03c4\u03bf\u03c5\u03c1\u03b3\u03b9\u03ba\u03bf\u03cd \u03c3\u03c5\u03c3\u03c4\u03ae\u03bc\u03b1\u03c4\u03bf\u03c2 Linux",
    "64bit-selectable_description": "\u0397 \u03b1\u03c0\u03cc\u03b4\u03bf\u03c3\u03b7 \u03c4\u03bf\u03c5 \u03c5\u03c0\u03bf\u03bb\u03bf\u03b3\u03b9\u03c3\u03c4\u03ae \u03bc\u03bf\u03c5 \u03b5\u03af\u03bd\u03b1\u03b9 \u03ba\u03b1\u03bb\u03ae, \u03b4\u03b5\u03bd \u03b1\u03bd\u03c4\u03b9\u03bc\u03b5\u03c4\u03c9\u03c0\u03af\u03b6\u03c9 \u03c0\u03c1\u03bf\u03b2\u03bb\u03ae\u03bc\u03b1\u03c4\u03b1 \u03b1\u03c0\u03cc\u03b4\u03bf\u03c3\u03b7\u03c2, \u03c4\u03bf \u03c5\u03bb\u03b9\u03ba\u03cc \u03bc\u03bf\u03c5 \u03c5\u03c0\u03bf\u03c3\u03c4\u03b7\u03c1\u03af\u03b6\u03b5\u03b9 64 bit.",
    "no64bit-selectable_description": "\u0394\u03b5\u03bd \u03b3\u03bd\u03c9\u03c1\u03af\u03b6\u03c9 \u03b7 \u03b4\u03b5\u03bd \u03b5\u03af\u03bc\u03b1\u03b9 \u03c3\u03af\u03b3\u03bf\u03c5\u03c1\u03bf\u03c2 \u03b1\u03bc\u03b1 \u03bf \u03c5\u03c0\u03bf\u03bb\u03bf\u03b3\u03b9\u03c3\u03c4\u03ae\u03c2 \u03bc\u03bf\u03c5 \u03b5\u03af\u03bd\u03b1\u03b9 32 Bit \u03c0\u03ac\u03bd\u03c4\u03c9\u03c2 \u03b5\u03bc\u03c6\u03b1\u03bd\u03af\u03b6\u03b5\u03b9 \u03c0\u03c1\u03bf\u03b2\u03bb\u03ae\u03bc\u03b1\u03c4\u03b1 \u03c4\u03b1\u03c7\u03cd\u03c4\u03b7\u03c4\u03b1\u03c2.",
    "beginner-usage-selectable_description": "\u03a7\u03c1\u03b5\u03b9\u03ac\u03b6\u03bf\u03bc\u03b1\u03b9 \u03c3\u03c5\u03c7\u03bd\u03ac \u03b2\u03bf\u03ae\u03b8\u03b5\u03b9\u03b1 \u03b1\u03c0\u03cc \u03ac\u03bb\u03bb\u03bf\u03c5\u03c2 \u03b3\u03b9\u03b1 \u03bd\u03b1 \u03b4\u03b9\u03bf\u03c1\u03b8\u03ce\u03c3\u03c9 \u03c0\u03c1\u03bf\u03b2\u03bb\u03ae\u03bc\u03b1\u03c4\u03b1 \u03bc\u03b5 \u03c4\u03bf\u03bd \u03c5\u03c0\u03bf\u03bb\u03bf\u03b3\u03b9\u03c3\u03c4\u03ae \u03bc\u03bf\u03c5",
    "advanced-usage-selectable_description": "\u0395\u03af\u03bc\u03b1\u03b9 \u03ae\u03b4\u03b7 \u03c3\u03b5 \u03b8\u03ad\u03c3\u03b7 \u03bd\u03b1 \u03bb\u03cd\u03bd\u03c9 \u03bc\u03cc\u03bd\u03bf\u03c2 \u03bc\u03bf\u03c5 \u03bf\u03c1\u03b9\u03c3\u03bc\u03ad\u03bd\u03b1 \u03c0\u03c1\u03bf\u03b2\u03bb\u03ae\u03bc\u03b1\u03c4\u03b1 \u03c4\u03bf\u03c5 \u03c5\u03c0\u03bf\u03bb\u03bf\u03b3\u03b9\u03c3\u03c4\u03ae \u03bc\u03bf\u03c5.",
    "professional-usage-selectable_description": "\u039c\u03c0\u03bf\u03c1\u03ce \u03bd\u03b1 \u03b5\u03c0\u03b9\u03bb\u03cd\u03c3\u03c9 \u03c4\u03b1 \u03c0\u03b5\u03c1\u03b9\u03c3\u03c3\u03cc\u03c4\u03b5\u03c1\u03b1 \u03ae \u03cc\u03bb\u03b1 \u03c4\u03b1 \u03c0\u03c1\u03bf\u03b2\u03bb\u03ae\u03bc\u03b1\u03c4\u03b1 \u03c4\u03bf\u03c5 \u03c5\u03c0\u03bf\u03bb\u03bf\u03b3\u03b9\u03c3\u03c4\u03ae \u03bc\u03cc\u03bd\u03bf\u03c2 \u03bc\u03bf\u03c5",
    "pricing-free-selectable_description": "\u03a0\u03c1\u03bf\u03c4\u03b9\u03bc\u03ce \u03bc\u03b9\u03b1 \u03b4\u03c9\u03c1\u03b5\u03ac\u03bd \u03b4\u03b9\u03b1\u03bd\u03bf\u03bc\u03ae",
    "paid-support-ok-selectable_description": "\u0398\u03b1 \u03ae\u03bc\u03bf\u03c5\u03bd \u03b5\u03bd\u03c4\u03ac\u03be\u03b5\u03b9 \u03bd\u03b1 \u03c0\u03bb\u03b7\u03c1\u03ce\u03c3\u03c9 \u03bc\u03b9\u03b1 \u03b1\u03bc\u03bf\u03b9\u03b2\u03ae",
    "privacy-no-connections-unless-wanted-selectable_description": "\u0394\u03b5\u03bd \u03c4\u03bf \u03b8\u03ad\u03bb\u03c9 \u03b1\u03c5\u03c4\u03cc",
    "okay-when-working-selectable_description": "\u0395\u03ac\u03bd \u03b2\u03b5\u03bb\u03c4\u03b9\u03ce\u03bd\u03b5\u03b9 \u03c4\u03b7\u03bd \u03b5\u03bc\u03c0\u03b5\u03b9\u03c1\u03af\u03b1 \u03bc\u03bf\u03c5 \u03c9\u03c2 \u03c7\u03c1\u03ae\u03c3\u03c4\u03b7\u03c2, \u03c3\u03c5\u03bc\u03c6\u03c9\u03bd\u03ce \u03bc\u03b5 \u03c4\u03b7 \u03c7\u03c1\u03ae\u03c3\u03b7 \u03c4\u03ad\u03c4\u03bf\u03b9\u03c9\u03bd \u03b4\u03b9\u03b1\u03b4\u03b9\u03ba\u03c4\u03c5\u03b1\u03ba\u03ce\u03bd \u03c5\u03c0\u03b7\u03c1\u03b5\u03c3\u03b9\u03ce\u03bd.",
    "daily-usage-selectable_description": "\u0398\u03ad\u03bb\u03c9 \u03bc\u03b9\u03b1 \u03b4\u03b9\u03b1\u03bd\u03bf\u03bc\u03ae \u03b3\u03b9\u03b1 \u03ba\u03b1\u03b8\u03b7\u03bc\u03b5\u03c1\u03b9\u03bd\u03ae \u03c7\u03c1\u03ae\u03c3\u03b7.",
    "privacy-usage-selectable_description": "\u0398\u03ad\u03bb\u03c9 \u03bd\u03b1 \u03c7\u03c1\u03b7\u03c3\u03b9\u03bc\u03bf\u03c0\u03bf\u03b9\u03ae\u03c3\u03c9 \u03c4\u03bf Linux \u03b3\u03b9\u03b1 \u03b1\u03bd\u03ce\u03bd\u03c5\u03bc\u03b7 \u03c0\u03b5\u03c1\u03b9\u03ae\u03b3\u03b7\u03c3\u03b7 \u03c3\u03c4\u03bf \u03b4\u03b9\u03b1\u03b4\u03af\u03ba\u03c4\u03c5\u03bf.",
    "gaming-usage-selectable_description": "\u03a0\u03c1\u03bf\u03c4\u03b9\u03bc\u03ce \u03bc\u03b9\u03b1 \u03b4\u03b9\u03b1\u03bd\u03bf\u03bc\u03ae \u03c0\u03bf\u03c5 \u03c5\u03c0\u03bf\u03c3\u03c4\u03b7\u03c1\u03af\u03b6\u03b5\u03c4\u03b1\u03b9 \u03b1\u03c0\u03cc \u03c4\u03bf\u03c5\u03c2 \u03b5\u03ba\u03b4\u03cc\u03c4\u03b5\u03c2 \u03c0\u03b1\u03b9\u03c7\u03bd\u03b9\u03b4\u03b9\u03ce\u03bd.",
    "privacy-isolation-usage-selectable_description": "\u0398\u03ad\u03bb\u03c9 \u03bd\u03b1 \u03b5\u03ba\u03c4\u03b5\u03bb\u03ad\u03c3\u03c9 \u03cc\u03bb\u03b1 \u03c4\u03b1 \u03c0\u03c1\u03bf\u03b3\u03c1\u03ac\u03bc\u03bc\u03b1\u03c4\u03b1 \u03c3\u03b5 \u03bc\u03b9\u03b1 \u03b1\u03c0\u03bf\u03bc\u03bf\u03bd\u03c9\u03bc\u03ad\u03bd\u03b7 \u03c0\u03b5\u03c1\u03b9\u03bf\u03c7\u03ae",
    "impaired-view-usage-selectable_description": "\u039f \u03c7\u03c1\u03ae\u03c3\u03c4\u03b7\u03c2 \u03ad\u03c7\u03b5\u03b9 \u03c0\u03c1\u03bf\u03b2\u03bb\u03ae\u03bc\u03b1\u03c4\u03b1 \u03cc\u03c1\u03b1\u03c3\u03b7\u03c2",
    "live-mode-usage-selectable_description": "\u0394\u03b9\u03b1\u03b8\u03ad\u03c4\u03b5\u03b9 \u03b4\u03bf\u03ba\u03b9\u03bc\u03b1\u03c3\u03c4\u03b9\u03ba\u03ae \u03bb\u03b5\u03b9\u03c4\u03bf\u03c5\u03c1\u03b3\u03af\u03b1: \u03b3\u03b9\u03b1 \u03bd\u03b1 \u03c4\u03bf \u03b4\u03bf\u03ba\u03b9\u03bc\u03ac\u03c3\u03b5\u03c4\u03b5 \u03b1\u03c0\u03bf \u03bf\u03c0\u03c4\u03b9\u03ba\u03bf\u03cd\u03c2 \u03b4\u03af\u03c3\u03ba\u03bf\u03c5\u03c2 \u03b7 USB Stick (\u03c0\u03c1\u03b9\u03bd \u03b1\u03c0\u03cc \u03c4\u03b7\u03bd \u03b5\u03b3\u03ba\u03b1\u03c4\u03ac\u03c3\u03c4\u03b1\u03c3\u03b7)",
    "scope-out-of-the-box-selectable_description": "\u03a0\u03c1\u03bf\u03c4\u03b9\u03bc\u03ce \u03bc\u03b9\u03b1 \u03b4\u03b9\u03b1\u03bd\u03bf\u03bc\u03ae \u03c0\u03bf\u03c5 \u03bd\u03b1 \u03b5\u03c7\u03b5\u03b9 \u03cc\u03bb\u03b1 \u03c4\u03b1 \u03b2\u03b1\u03c3\u03b9\u03ba\u03ac \u03c0\u03c1\u03bf\u03b3\u03c1\u03ac\u03bc\u03bc\u03b1\u03c4\u03b1 \u03c0\u03bf\u03c5 \u03c7\u03c1\u03b5\u03b9\u03ac\u03b6\u03bf\u03bc\u03b1\u03b9 \u03b3\u03b9\u03b1 \u03bd\u03b1 \u03b1\u03c1\u03c7\u03af\u03c3\u03c9 \u03bd\u03b1 \u03b5\u03c1\u03b3\u03ac\u03b6\u03bf\u03bc\u03b1\u03b9 \u03b1\u03bc\u03ad\u03c3\u03c9\u03c2 \u03bc\u03b5\u03c4\u03ac \u03c4\u03b7\u03bd \u03b5\u03b3\u03ba\u03b1\u03c4\u03ac\u03c3\u03c4\u03b1\u03c3\u03b7",
    "scope-own-selection-selectable_description": "\u0398\u03ad\u03bb\u03c9 \u03bd\u03b1 \u03b5\u03c0\u03b9\u03bb\u03ad\u03be\u03c9 \u03bc\u03cc\u03bd\u03bf\u03c2 \u03bc\u03bf\u03c5 \u03c4\u03b1 \u03b2\u03b1\u03c3\u03b9\u03ba\u03ac \u03c0\u03c1\u03bf\u03b3\u03c1\u03ac\u03bc\u03bc\u03b1\u03c4\u03b1 \u03c0\u03bf\u03c5 \u03b8\u03b1 \u03b5\u03b3\u03ba\u03b1\u03c4\u03b1\u03c3\u03c4\u03ae\u03c3\u03c9",
    "prefer-stable-selectable_description": "\u03a0\u03c1\u03bf\u03c4\u03b9\u03bc\u03ce \u03c4\u03b9\u03c2 \u03c3\u03c4\u03b1\u03b8\u03b5\u03c1\u03ad\u03c2 \u03b5\u03bd\u03b7\u03bc\u03b5\u03c1\u03ce\u03c3\u03b5\u03b9\u03c2",
