{
    "app-store-gui-selectable_description": "\u05d1\u05e8\u05e6\u05d5\u05e0\u05d9 \u05dc\u05d4\u05ea\u05e7\u05d9\u05df \u05d0\u05ea \u05d4\u05ea\u05db\u05e0\u05d9\u05d5\u05ea \u05e9\u05dc\u05d9 \u05d1\u05e2\u05d6\u05e8\u05ea \"\u05d7\u05e0\u05d5\u05ea \u05d9\u05d9\u05e9\u05d5\u05de\u05d9\u05dd\"",
    "no-systemd-selectable_description": "\u05d1\u05e8\u05e6\u05d5\u05e0\u05d9 \u05dc\u05d4\u05d9\u05de\u05e0\u05e2 \u05de\u05e9\u05d9\u05de\u05d5\u05e9 \u05d1\u05besystemd",
    "manuals-selectable_description": "\u05d0\u05e0\u05d9 \u05d9\u05db\u05d5\u05dc/\u05d4 \u05dc\u05e4\u05ea\u05d5\u05e8 \u05d1\u05e2\u05e6\u05de\u05d9 \u05d0\u05ea \u05d4\u05d1\u05e2\u05d9\u05d5\u05ea \u05d1\u05de\u05d7\u05e9\u05d1 \u05d1\u05e2\u05d6\u05e8\u05ea \u05de\u05d3\u05e8\u05d9\u05db\u05d9\u05dd \u05de\u05d3\u05e4\u05d9 \u05d5\u05d9\u05e7\u05d9 \u05d0\u05d5 \u05de\u05d3\u05e8\u05d9\u05db\u05d9\u05dd",
    "need-human-help-selectable_description": "\u05d0\u05e0\u05d9 \u05de\u05e2\u05d3\u05d9\u05e3/\u05d4 \u05dc\u05d1\u05e7\u05e9 \u05e2\u05d6\u05e8\u05d4 \u05de\u05d0\u05d7\u05e8\u05d9\u05dd",
    "license-open-source-selectable_description": "\u05d1\u05e8\u05e6\u05d5\u05e0\u05d9 \u05dc\u05d4\u05e9\u05ea\u05de\u05e9 \u05d1\u05db\u05de\u05d4 \u05e9\u05d9\u05d5\u05ea\u05e8 \u05e8\u05d9\u05e9\u05d9\u05d5\u05e0\u05d5\u05ea \u05d7\u05d5\u05e4\u05e9\u05d9\u05d9\u05dd",
    "proprietary-okay-when-working-selectable_description": "\u05d0\u05e0\u05d9 \u05d1\u05e1\u05d3\u05e8 \u05e2\u05dd \u05e8\u05d9\u05e9\u05d9\u05d5\u05e0\u05d5\u05ea \u05e9\u05d0\u05d9\u05e0\u05dd \u05d7\u05d5\u05e4\u05e9\u05d9\u05d9\u05dd \u05db\u05dc \u05e2\u05d5\u05d3 \u05d4\u05de\u05e2\u05e8\u05db\u05ea \u05e9\u05dc\u05d9 \u05e2\u05d5\u05d1\u05d3\u05ea",
    "many-preselections-selectable_description": "\u05d1\u05e8\u05e6\u05d5\u05e0\u05d9 \u05dc\u05d4\u05e9\u05ea\u05de\u05e9 \u05d1\u05de\u05e2\u05e8\u05db\u05d9 \u05d4\u05d4\u05d2\u05d3\u05e8\u05d5\u05ea \u05d1\u05e8\u05d9\u05e8\u05ea\u05be\u05d4\u05de\u05d7\u05d3\u05dc \u05e9\u05dc \u05d0\u05e9\u05e3 \u05d4\u05d4\u05ea\u05e7\u05e0\u05d4",
    "commandline-setup-selectable_description": "\u05d1\u05e8\u05e6\u05d5\u05e0\u05d9 \u05dc\u05d1\u05d7\u05d5\u05e8 \u05d0\u05ea \u05d4\u05d4\u05d2\u05d3\u05e8\u05d5\u05ea \u05d1\u05e2\u05e6\u05de\u05d9",
    "many-choices-gui-selectable_description": "\u05d1\u05e8\u05e6\u05d5\u05e0\u05d9 \u05dc\u05d4\u05d2\u05d3\u05d9\u05e8 \u05db\u05de\u05d4 \u05d9\u05d5\u05ea\u05e8 \u05d3\u05d1\u05e8\u05d9\u05dd \u05e9\u05d0\u05e4\u05e9\u05e8 \u05d1\u05d0\u05de\u05e6\u05e2\u05d5\u05ea \u05de\u05de\u05e9\u05e7 \u05de\u05e9\u05ea\u05de\u05e9 \u05d2\u05e8\u05e4\u05d9",
    "no-linux-contact-selectable_description": "\u05d9\u05e9 \u05dc\u05d9 \u05d9\u05d3\u05e2 \u05e7\u05d8\u05df \u05d0\u05d5 \u05d0\u05e4\u05e1\u05d9 \u05d1\u05dc\u05d9\u05e0\u05d5\u05e7\u05e1",
    "already-used-selectable_description": "\u05db\u05d1\u05e8 \u05d4\u05e9\u05ea\u05de\u05e9\u05ea\u05d9 \u05d1\u05dc\u05d9\u05e0\u05d5\u05e7\u05e1 \u05dc\u05de\u05e1\u05e4\u05e8 \u05de\u05d8\u05e8\u05d5\u05ea",
    "knowledge-present-selectable_description": "\u05d9\u05e9 \u05dc\u05d9 \u05d4\u05d1\u05e0\u05d4 \u05d8\u05d5\u05d1\u05d4 \u05d1\u05de\u05e2\u05e8\u05db\u05ea \u05d4\u05d4\u05e4\u05e2\u05dc\u05d4 \u05dc\u05d9\u05e0\u05d5\u05e7\u05e1",
    "64bit-selectable_description": "\u05d1\u05d9\u05e6\u05d5\u05e2\u05d9 \u05d4\u05de\u05d7\u05e9\u05d1 \u05e9\u05dc\u05d9 \u05d8\u05d5\u05d1\u05d9\u05dd, \u05dc\u05d0 \u05d7\u05d5\u05d5\u05d9\u05ea\u05d9 \u05db\u05dc \u05d1\u05e2\u05d9\u05d5\u05ea \u05d1\u05d9\u05e6\u05d5\u05e2\u05d9\u05dd \u05e9\u05d4\u05df, \u05d5\u05d4\u05d7\u05d5\u05de\u05e8\u05d4 \u05e9\u05dc\u05d9 \u05ea\u05d5\u05de\u05db\u05ea \u05d1\u05be64 \u05e1\u05d9\u05d1\u05d9\u05d5\u05ea",
    "no64bit-selectable_description": "\u05d0\u05d9\u05e0\u05e0\u05d9 \u05d1\u05d8\u05d5\u05d7/\u05d4 \u05dc\u05d2\u05d1\u05d9 \u05d6\u05d4 \u05d5/\u05d0\u05d5 \u05dc\u05de\u05d7\u05e9\u05d1\u05d9 \u05d9\u05e9 \u05d1\u05e2\u05d9\u05d5\u05ea \u05d1\u05d9\u05e6\u05d5\u05e2\u05d9\u05dd \u05d5/\u05d0\u05d5 \u05ea\u05d5\u05de\u05da \u05e8\u05e7 \u05d1\u05be32 \u05e1\u05d9\u05d1\u05d9\u05d5\u05ea",
    "beginner-usage-selectable_description": "\u05dc\u05e8\u05d5\u05d1 \u05d9\u05e9 \u05dc\u05d9 \u05e6\u05d5\u05e8\u05da \u05d1\u05e2\u05d6\u05e8\u05d4 \u05de\u05d0\u05d7\u05e8\u05d9\u05dd \u05d1\u05ea\u05d9\u05e7\u05d5\u05df \u05d4\u05d1\u05e2\u05d9\u05d5\u05ea \u05d1\u05de\u05d7\u05e9\u05d1",
    "advanced-usage-selectable_description": "\u05db\u05d1\u05e8 \u05e4\u05ea\u05e8\u05ea\u05d9 \u05d1\u05e2\u05e6\u05de\u05d9 \u05de\u05e1\u05e4\u05e8 \u05d1\u05e2\u05d9\u05d5\u05ea \u05d1\u05de\u05d7\u05e9\u05d1",
    "professional-usage-selectable_description": "\u05d0\u05e0\u05d9 \u05d9\u05db\u05d5\u05dc/\u05d4 \u05dc\u05e4\u05ea\u05d5\u05e8 \u05d1\u05e2\u05e6\u05de\u05d9 \u05d0\u05ea \u05e8\u05d5\u05d1 \u05d0\u05d5 \u05d0\u05ea \u05db\u05dc \u05d4\u05d1\u05e2\u05d9\u05d5\u05ea \u05d1\u05de\u05d7\u05e9\u05d1",
    "pricing-free-selectable_description": "\u05d0\u05e0\u05d9 \u05de\u05e2\u05d3\u05d9\u05e3/\u05d4 \u05d4\u05e4\u05e6\u05d4 \u05d1\u05d7\u05d9\u05e0\u05dd",
    "paid-support-ok-selectable_description": "\u05d0\u05d9\u05df \u05dc\u05d9 \u05d1\u05e2\u05d9\u05d4 \u05e2\u05dd \u05ea\u05e9\u05dc\u05d5\u05dd \u05e1\u05de\u05dc\u05d9",
    "privacy-no-connections-unless-wanted-selectable_description": "\u05d0\u05d9\u05e0\u05e0\u05d9 \u05e8\u05d5\u05e6\u05d4 \u05d0\u05ea \u05d6\u05d4",
    "okay-when-working-selectable_description": "\u05d0\u05dd \u05d6\u05d4 \u05de\u05e9\u05e4\u05e8 \u05d0\u05ea \u05d7\u05d5\u05d5\u05d9\u05d9\u05ea \u05d4\u05de\u05e9\u05ea\u05de\u05e9 \u05e9\u05dc\u05d9, \u05d0\u05e0\u05d9 \u05de\u05e1\u05db\u05d9\u05dd/\u05d4 \u05dc\u05d4\u05e9\u05ea\u05de\u05e9 \u05d1\u05e9\u05d9\u05e8\u05d5\u05ea\u05d9\u05dd \u05de\u05e7\u05d5\u05d5\u05e0\u05d9\u05dd \u05db\u05d0\u05dc\u05d5",
    "daily-usage-selectable_description": "\u05d0\u05e0\u05d9 \u05e8\u05d5\u05e6\u05d4 \u05d4\u05e4\u05e6\u05d4 \u05dc\u05e9\u05d9\u05de\u05d5\u05e9 \u05d9\u05d5\u05de\u05d9\u05d5\u05de\u05d9.",
    "privacy-usage-selectable_description": "\u05d0\u05e0\u05d9 \u05e8\u05d5\u05e6\u05d4 \u05dc\u05d4\u05e9\u05ea\u05de\u05e9 \u05d1\u05dc\u05d9\u05e0\u05d5\u05e7\u05e1 \u05dc\u05de\u05d8\u05e8\u05d5\u05ea \u05d2\u05dc\u05d9\u05e9\u05d4 \u05d0\u05e0\u05d5\u05e0\u05d9\u05de\u05d9\u05ea \u05d1\u05e8\u05e9\u05ea.",
    "gaming-usage-selectable_description": "\u05d0\u05e0\u05d9 \u05de\u05e2\u05d3\u05d9\u05e3/\u05d4 \u05d4\u05e4\u05e6\u05d4 \u05e9\u05e0\u05ea\u05de\u05db\u05ea \u05e2\u05dc \u05d9\u05d3\u05d9 \u05de\u05e4\u05ea\u05d7\u05d9 \u05de\u05e9\u05d7\u05e7\u05d9\u05dd.",
    "privacy-isolation-usage-selectable_description": "\u05d1\u05e8\u05e6\u05d5\u05e0\u05d9 \u05dc\u05d4\u05e4\u05e2\u05d9\u05dc \u05d0\u05ea \u05db\u05dc \u05d4\u05ea\u05d5\u05db\u05e0\u05d9\u05d5\u05ea \u05d1\u05d0\u05d6\u05d5\u05e8\u05d9\u05dd \u05de\u05d1\u05d5\u05d3\u05d3\u05d9\u05dd",
    "impaired-view-usage-selectable_description": "\u05dc\u05de\u05e9\u05ea\u05de\u05e9/\u05ea \u05dc\u05e7\u05d5\u05ea \u05e8\u05d0\u05d9\u05d9\u05d4",
    "live-mode-usage-selectable_description": "\u05e2\u05dd \u05de\u05e6\u05d1 \u05d4\u05de\u05d7\u05e9\u05d4: \u05e0\u05d9\u05ea\u05df \u05dc\u05d4\u05e4\u05e2\u05d9\u05dc\u05d4 \u05de\u05ea\u05d5\u05da \u05ea\u05e7\u05dc\u05d9\u05d8\u05d5\u05e8\u05d9\u05dd \u05d0\u05d5 \u05d4\u05d7\u05e1\u05e0\u05d9\u05dd \u05e0\u05d9\u05d9\u05d3\u05d9\u05dd (USB) \u05db\u05d3\u05d9 \u05dc\u05d1\u05d3\u05d5\u05e7 \u05d0\u05d5\u05ea\u05d4 (\u05dc\u05e4\u05e0\u05d9 \u05d4\u05d4\u05ea\u05e7\u05e0\u05d4)",
    "scope-out-of-the-box-selectable_description": "\u05d0\u05e0\u05d9 \u05de\u05e2\u05d3\u05d9\u05e3/\u05d4 \u05d4\u05e4\u05e6\u05d4 \u05e9\u05de\u05d2\u05d9\u05e2\u05d4 \u05e2\u05dd \u05db\u05dc \u05d4\u05ea\u05db\u05e0\u05d9\u05d5\u05ea \u05d4\u05d1\u05e1\u05d9\u05e1\u05d9\u05d5\u05ea \u05e9\u05d0\u05e0\u05d9 \u05e6\u05e8\u05d9\u05da/\u05d4 \u05db\u05d3\u05d9 \u05dc\u05d4\u05ea\u05d7\u05d9\u05dc \u05dc\u05e2\u05d1\u05d5\u05d3 \u05de\u05d9\u05d3 \u05dc\u05d0\u05d7\u05e8 \u05d4\u05d4\u05ea\u05e7\u05e0\u05d4",
    "scope-own-selection-selectable_description": "\u05d0\u05e0\u05d9 \u05de\u05e2\u05d3\u05d9\u05e3/\u05d4 \u05dc\u05d1\u05d7\u05d5\u05e8 \u05d1\u05e2\u05e6\u05de\u05d9 \u05d0\u05ea \u05d4\u05ea\u05db\u05e0\u05d9\u05d5\u05ea \u05d4\u05d1\u05e1\u05d9\u05e1\u05d9\u05d5\u05ea \u05dc\u05d4\u05ea\u05e7\u05e0\u05d4",
    "prefer-stable-selectable_description": "\u05d0\u05e0\u05d9 \u05de\u05e2\u05d3\u05d9\u05e3/\u05d4 \u05e2\u05d3\u05db\u05d5\u05e0\u05d9\u05dd \u05d9\u05e6\u05d9\u05d1\u05d9\u05dd",
    "prefer-fast-selectable_description": "\u05d0\u05e0\u05d9 \u05de\u05e2\u05d3\u05d9\u05e3/\u05d4 \u05e2\u05d3\u05db\u05d5\u05e0\u05d9\u05dd \u05de\u05d4\u05d9\u05e8\u05d9\u05dd",
    "ux-concept-answer-windows-like-selectable_description": "\u05d0\u05e0\u05d9 \u05de\u05e2\u05d3\u05d9\u05e3/\u05d4 \u05de\u05de\u05e9\u05e7 \u05de\u05e9\u05ea\u05de\u05e9 \u05d3\u05de\u05d5\u05d9\u05ea Windows",
    "ux-concept-answer-mac-like-selectable_description": "\u05d0\u05e0\u05d9 \u05de\u05e2\u05d3\u05d9\u05e3/\u05d4 \u05de\u05de\u05e9\u05e7 \u05de\u05e9\u05ea\u05de\u05e9 \u05d3\u05de\u05d5\u05d9 macOS",
    "software-admin-answer-console-selectable_description": "\u05d1\u05e8\u05e6\u05d5\u05e0\u05d9 \u05dc\u05d4\u05ea\u05e7\u05d9\u05df \u05ea\u05d5\u05db\u05e0\u05d4 \u05d1\u05e2\u05d9\u05e7\u05e8 \u05d1\u05d0\u05de\u05e6\u05e2\u05d5\u05ea \u05e4\u05e7\u05d5\u05d3\u05d5\u05ea \u05d1\u05de\u05e1\u05d5\u05e3"
}